extends user-layout

block cust-css
    link(rel='stylesheet', href='/css/jstree.style.css')
    link(rel='stylesheet', href='/css/toastr.css')

block content
    if displayForm
        .row.main
            .col-md-7
                .portlet.light.bordered
                    .portlet-title
                        .caption
                            span.caption-subject.font-green-sharp.bold.uppercase= displayForm.name + " - 表单实例"
                        .actions
                            - if (displayForm.perms && displayForm.perms.operate)
                              a.btn.btn-circle.btn-danger.stopBtn(href="#")
                                i.fa.fa-ban &nbsp;终止
                              a.btn.btn-circle.btn-danger.arrangeBtn(href="#")
                                i.fa.fa-gear &nbsp;安排到机台
                            - if (displayForm.perms && displayForm.perms.approve)
                              a.btn.btn-circle.btn-danger.submitBtn(href="#")
                                i.fa.fa-arrow-circle-down &nbsp;审核并下发
                    .portlet-body
                        .dataTables_wrapper.no-footer
                            table.table.table-striped.table-hover.table-bordered.dataTable.no-footer(role="grid")
                                thead
                                    tr(role="row")
                                        th.sorting(tabindex="0", rowspan="1", colspan="1")
                                        th.sorting(tabindex="0", rowspan="1", colspan="1") 绳规格-绳直径
                                        th.sorting(tabindex="0", rowspan="1", colspan="1") 类别
                                        th.sorting(tabindex="0", rowspan="1", colspan="1") 计划开始
                                        th.sorting(tabindex="0", rowspan="1", colspan="1") 计划结束
                                        th.sorting(tabindex="0", rowspan="1", colspan="1") 车间
                                        th.sorting(tabindex="0", rowspan="1", colspan="1") 机台号
                                        th.sorting(tabindex="0", rowspan="1", colspan="1") 班组
                                        th.sorting(tabindex="0", rowspan="1", colspan="1") 班长
                                        th.sorting(tabindex="0", rowspan="1", colspan="1") 操作工
                                        th.sorting(tabindex="0", rowspan="1", colspan="1") 状态
            .col-md-5.form-card(id=displayForm._id)
                .portlet.light.bordered
                    .portlet-title
                        .caption
                            span.caption-subject.font-green-sharp.bold.uppercase= displayForm.name
                            span.caption-helper
                        .actions
                            a.btn.btn-outline.btn-circle.expand.hidden(href="#")
                                i.fa.fa-expand &nbsp;展开
                            a.btn.btn-outline.btn-circle.compress.hidden(href="#")
                                i.fa.fa-compress &nbsp;收起
                    .portlet-body
                        .tree-container
                            .tree-actions.hidden
                            .tree-body
                    .portlet-footer
                        .tree-header.form-inline.hidden
                            .row
                                .form-group.col-md-6
                                    label 制表
                                    input.form-control(type='text', name='author', disabled)
                                .form-group.col-md-6
                                    label 制表日期
                                    input.form-control(type='text', name='created', disabled)
                            .row
                                .form-group.col-md-6
                                    label 审核
                                    input.form-control(type='text', name='auditor', disabled)
                                .form-group.col-md-6
                                    label 审核日期
                                    input.form-control(type='text', name='audit_date', disabled)
                            .row
                                .portlet-actions.pull-right(style="margin-right:14px")
                                    button.cancel.btn.btn-default 取消
                                    - if (displayForm.perms && displayForm.perms.operate)
                                      button.delete.btn.btn-danger 删除
        .modal.arrange
            .modal-dialog(style="width:86%")
                form.form-horizontal(role="form")
